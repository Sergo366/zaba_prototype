#!groovy
//Build, Test, Prod
pipeline {
    agent any

    triggers {
        pollSCM('*/5 * * * 1-5')
    }

    options {
        skipDefaultCheckout(true)
        // Keep the 10 most recent builds
        buildDiscarder(logRotator(numToKeepStr: '10'))
        timestamps()
    }


stages {

        stage ("Code pull"){
            steps{
                checkout scm
            }
        }

}




}